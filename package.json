{
  "name": "@dinodns/redis-store",
  "version": "0.1.9",
  "description": "A Redis/Valkey storage plugin for DinoDNS",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc -b -v",
    "build:clean": "rm -rf dist && tsc -b -v --incremental false",
    "dev": "nodemon",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs:build": "typedoc --out docs ./src",
    "docs:clean": "rm -rf docs",
    "docs:serve": "npx http-server docs -o",
    "lint": "eslint",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "postpublish": "git push --follow-tags",
    "release": "npm run build:clean && cp package.json dist/package.json && cp README.md dist/README.md && cd dist && npm publish",
    "package": "npm run build:clean && cp package.json dist/package.json && cp README.md dist/README.md && cd dist && npm pack"
  },
  "keywords": [],
  "author": "Josh Fayer <contact@fayer.me>",
  "license": "ISC",
  "devDependencies": {
    "@types/dns-packet": "^5.6.5",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rollup": "^2.79.1",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "tsx": "^4.10.5",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@types/lodash": "^4.17.10",
    "dinodns": "^0.0.11",
    "dns-packet": "^5.6.1",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "typescript-eslint": "^8.8.0"
  },
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jafayer/dinodns-redis-storage.git"
  },
  "bugs": {
    "url": "https://github.com/jafayer/dinodns-redis-storage/issues"
  },
  "homepage": "https://github.com/jafayer/dinodns-redis-storage#readme"
}
